{
    "version": 2,
    "builds": [
      {
        "src": "api/index.js",
        "use": "@vercel/node"
      }
    ],
    "rewrites": [
      { "source": "/auth/(.*)", "destination": "/api/index.js" },
      { "source": "/itineraries/(.*)", "destination": "/api/index.js" },
      { "source": "/(.*)", "destination": "/api/index.js" }
    ]
  }
  